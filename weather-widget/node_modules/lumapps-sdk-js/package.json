{
    "name": "lumapps-sdk-js",
    "version": "1.0.2",
    "description": "SDK For LumApps intranet",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "repository": "https://github.com/lumapps/lumapps-sdk-js.git",
    "license": "MIT",
    "files": [
        "dist/",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "yarn run build:library && yarn run build:types",
        "build:library": "webpack --config webpack.config.js",
        "build:types": "tsc --build tsconfig.build.json",
        "build:storybook": "build-storybook",
        "check:typing": "tsc --build tsconfig.json",
        "check:lint": "eslint --ext .ts,.tsx,.js,.jsx \"src/\"",
        "clean": "rm -rf dist",
        "coverage": "jest --coverage",
        "prepack": "yarn run build",
        "semantic-release": "npx semantic-release",
        "storybook": "start-storybook -p 6006",
        "test": "jest"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "^7.15.4"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@lumx/icons": "^2.0.0",
        "@lumx/react": "^2.0.0",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@storybook/addon-actions": "^6.3.7",
        "@storybook/addon-essentials": "^6.3.7",
        "@storybook/addon-links": "^6.3.7",
        "@storybook/react": "^6.3.7",
        "@types/babel-core": "^6.25.6",
        "@types/jest": "^27.0.1",
        "@types/react-redux": "^7.1.18",
        "@types/webpack": "5.28.0",
        "@types/whatwg-fetch": "^0.0.33",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "babel-loader": "^8.2.2",
        "css-loader": "^5.2.7",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^26.4.2",
        "moment": "^2.29.1",
        "moment-range": "^4.0.2",
        "prettier": "^2.3.2",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-redux": "^7.1.0",
        "reselect": "^4.0.0",
        "sass": "^1.39.0",
        "sass-loader": "^10.2.0",
        "semantic-release": "^17.4.7",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^5.1.4",
        "ts-jest": "^26.5.6",
        "typescript": "^4.4.2",
        "webpack": "5.42.0",
        "webpack-cli": "^4.7.2",
        "whatwg-fetch": "^3.6.2"
    },
    "peerDependencies": {
        "@lumx/icons": "^2.0.0",
        "@lumx/react": "^2.0.0",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-redux": "^7.1.0",
        "reselect": "^4.0.0",
